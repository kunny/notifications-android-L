<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidhuman.example.notifications" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LockscreenActivity"
            android:label="@string/title_activity_lockscreen"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".BridgeActivity"
            android:label="@string/title_activity_bridge"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".PriorityActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_priority"
            android:parentActivityName=".MainActivity"
            android:theme="@android:style/Theme.Material.NoActionBar.Fullscreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".ColorActivity"
            android:label="@string/title_activity_color"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".NotificationWithActionsActivity"
            android:label="@string/title_activity_notification_with_actions"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".VoiceInputActivity"
            android:label="@string/title_activity_voice_input"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".VoiceInputResultActivity"
            android:label="@string/title_activity_voice_input_result" >
        </activity>
        <activity
            android:name=".PagingActivity"
            android:label="@string/title_activity_paging"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
        <activity
            android:name=".StackActivity"
            android:label="@string/title_activity_stack"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.androidhuman.example.notifications.MainActivity" />
        </activity>
    </application>

</manifest>
